      "name": "Google Sheets - Read Regies",
      "type": "n8n-nodes-base.googleSheets",
      "typeVersion": 4.4,
      "position": [900, 400],
      "credentials": {
        "googleSheetsOAuth2Api": {
          "id": "Google_Sheets_ICS",
          "name": "Google_Sheets_ICS"
        }
      }
    },
    {
      "parameters": {
        "jsCode": "// Combiner les donnees\nconst extractedData = $('Code - Extract Details').first().json;\nconst employees = $('Google Sheets - Read Employees').all();\nconst regies = $('Google Sheets - Read Regies').all();\n\n// Trouver le nom du technicien\nlet technicianName = '';\nfor (const emp of employees) {\n  if (emp.json.email === extractedData.technician_email) {\n    technicianName = emp.json.name;\n    break;\n  }\n}\n\n// Trouver la regie\nlet regieName = '';\nlet regieEmail = '';\nfor (const regie of regies) {\n  if (regie.json.keyword?.toUpperCase() === extractedData.regie_keyword) {\n    regieName = regie.json.regie_name;\n    regieEmail = regie.json.regie_email;\n    break;\n  }\n}\n\nreturn [{\n  json: {\n    event_id: extractedData.event_id,\n    created_at: extractedData.created_at,\n    start_datetime: extractedData.start_datetime,\n    end_datetime: extractedData.end_datetime,\n    title: extractedData.title,\n    address: extractedData.address,\n    technician_name: technicianName,\n    technician_email: extractedData.technician_email,\n    regie_name: regieName,\n    regie_email: regieEmail,\n    confirmation_sent: 'FALSE'\n  }\n}];"
      },
