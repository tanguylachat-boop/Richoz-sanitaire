    {
      "parameters": {
        "jsCode": "// Extraire l'email technicien (premier @ics.ch dans attendees)\nconst item = $input.first().json;\nconst attendees = JSON.parse(item.attendees_raw || '[]');\n\nlet technicianEmail = '';\nfor (const attendee of attendees) {\n  if (attendee.email && attendee.email.includes('@ics.ch')) {\n    technicianEmail = attendee.email;\n    break;\n  }\n}\n\n// Extraire le keyword regie du titre [KEYWORD]\nconst title = item.title || '';\nconst keywordMatch = title.match(/\\[(.*?)\\]/);\nconst regieKeyword = keywordMatch ? keywordMatch[1].toUpperCase() : '';\n\nreturn [{\n  json: {\n    ...item,\n    technician_email: technicianEmail,\n    regie_keyword: regieKeyword\n  }\n}];"
      },
      "id": "code-extract-details",
      "name": "Code - Extract Details",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [680, 300]
    },
